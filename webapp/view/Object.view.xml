<mvc:View controllerName="xxxxrubby.ZTRAINING_FORM_ODATA.controller.Object" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<f:SimpleForm id="fSimpleForm" layout="ResponsiveGridLayout" editable="true" title="Display ID {Persno}" labelSpanXL="3" labelSpanL="3"
					labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1"
					columnsL="1" columnsM="1" singleContainerFullSize="false">
					<f:content >
						<Label text="{i18n>Personnel Number}" design="{i18n>labelBold}"></Label>
						<!--<Input id="idPersno" placeholder="Enter Personnel Number" maxLength="8" value="{Persno}" editable="false"></Input>-->
						<Text id="idPersno" text="{Persno}"/>
						<Input value="" visible="false"></Input>
						<Input value="" visible="false"></Input>
						<Input value="" visible="false"></Input>
						<Label text="{i18n>Gender}" design="{i18n>labelBold}"></Label>
						<RadioButtonGroup id="idGender" selectedIndex="{ path : 'Gender', formatter: '.formatter.changeGender' }">
							<RadioButton text="Male"></RadioButton>
							<RadioButton text="Female"></RadioButton>
						</RadioButtonGroup>
						<Label text="{i18n>searchHelpBlood}" design="{i18n>labelBold}"></Label>
						<Input id="idSearchHelpBlood" valueHelpRequest="onPressSearchHelpBlood" showValueHelp="true" valueHelpOnly="true" value="{BloodType}"></Input>
						<Label text="{i18n>Height}" design="{i18n>labelBold}"></Label>
						<Input id="idHeight" placeholder="Enter Height" maxLength="3"
							value="{ path : 'Height', type : 'sap.ui.model.odata.type.String', constraints: { isDigitSequence : true, maxLength : 5 } }"></Input>
						<Label text="{i18n>Weight}" design="{i18n>labelBold}"></Label>
						<Input id="idWeight" placeholder="Enter Weight" maxLength="3"
							value="{ path : 'Weight', type : 'sap.ui.model.odata.type.String', constraints: { isDigitSequence : true, maxLength : 5 } }"></Input>
						<Label text="{i18n>input}" design="{i18n>labelBold}"></Label>
						<Label text="{i18n>dateOfBirth}" design="{i18n>labelBold}"></Label>
						<DatePicker id="idDateOfBirth" required="true" change="onDateChanged" editable="false"></DatePicker>
						<Input id="idAge" editable="false"
							value="{ path : 'Age', type : 'sap.ui.model.odata.type.String', constraints: { isDigitSequence : true, maxLength : 5 } }"></Input>
						<!--Vaccine 1-->
						<Label text="{i18n>vaccineType1}" design="{i18n>labelBold}"></Label>
						<!--<Input id="idSearchHelpVaccine" valueHelpRequest="onPressSearchHelpVaccine" showValueHelp="true" valueHelpOnly="true" editable="true"></Input>-->
						<ComboBox id="idVaccine1" items="{/VacTypeShSet}" selectedKey="{Vac1Type}">
							<core:Item text="{VacTypeText}" key="{VacType}"></core:Item>
						</ComboBox>
						<Label text="{i18n>vaccineDate1}" design="{i18n>labelBold}"></Label>
						<DatePicker id="idVaccineDate1" required="true" value="{Vac1Date}"></DatePicker>
						<Label text="{i18n>vaccineLoc1}" design="{i18n>labelBold}"></Label>
						<Input id="idVaccineLoc1" valueHelpRequest="onPressVaccineLoc1" showValueHelp="true" valueHelpOnly="true" value="{LocationCode1}"></Input>
						<Label text="{i18n>vaccineOrg1}" design="{i18n>labelBold}"></Label>
						<Input id="idOrg1" placeholder="Enter Organization" value="{Vac1Org}"></Input>
						<Label text="{i18n>sideEffect1}" design="{i18n>labelBold}"></Label>
						<TextArea id="idSideEffect1" showExceededText="true" maxLength="250" liveChange="onLiveChangeTextArea" valueState="Error"
							valueStateText="Teu Kenging Kosong Jang" value="{Vac1SideEffect}"></TextArea>
						<!--Vaccine 2-->
						<Label text="{i18n>vaccineType1}" design="{i18n>labelBold}"></Label>
						<!--<Input id="idSearchHelpVaccine" valueHelpRequest="onPressSearchHelpVaccine" showValueHelp="true" valueHelpOnly="true" editable="true"></Input>-->
						<ComboBox id="idVaccine2" items="{/VacTypeShSet}" selectedKey="{Vac2Type}">
							<core:Item text="{VacTypeText}" key="{VacType}"></core:Item>
						</ComboBox>
						<Label text="{i18n>vaccineDate2}" design="{i18n>labelBold}"></Label>
						<DatePicker id="idVaccineDate2" required="true" value="{Vac2Date}"></DatePicker>
						<Label text="{i18n>vaccineLoc2}" design="{i18n>labelBold}"></Label>
						<Input id="idVaccineLoc2" valueHelpRequest="onPressVaccineLoc2" showValueHelp="true" valueHelpOnly="true" value="{LocationCode2}"></Input>
						<Label text="{i18n>vaccineOrg1}" design="{i18n>labelBold}"></Label>
						<Input id="idOrg2" placeholder="Enter Organization" value="{Vac2Org}"></Input>
						<Label text="{i18n>sideEffect1}" design="{i18n>labelBold}"></Label>
						<TextArea id="idSideEffect2" showExceededText="true" maxLength="250" liveChange="onLiveChangeSideEffect2" valueState="Error"
							valueStateText="Teu Kenging Kosong Jang" value="{Vac2SideEffect}"></TextArea>
						<Label text="{i18n>note}" design="{i18n>labelBold}"></Label>
						<TextArea id="idNote" showExceededText="true" maxLength="500" liveChange="onLiveChangeNote" valueState="Error"
							valueStateText="Teu Kenging Kosong Jang" value="{Note}"></TextArea>
						<!--xmlns:f="sap.ui.layout.form"-->
					</f:content>
				</f:SimpleForm>
				<content></content>
				<footer>
					<Toolbar>
						<ToolbarSpacer></ToolbarSpacer>
						<Button text="{i18n>Update}" type="Accept" press="onUpdate"></Button>
						<Button text="{i18n>Delete}" type="Reject" press="onDelete"></Button>
					</Toolbar>
				</footer>
			</Page>
		</pages>
	</App>
</mvc:View>
